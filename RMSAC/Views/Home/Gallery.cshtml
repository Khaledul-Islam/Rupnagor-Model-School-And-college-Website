
<style>
    * {
        box-sizing: border-box;
    }

    body {
        margin: 0;
        font-family: Arial;
    }

    .header {
        text-align: center;
        padding: 32px;
    }

    /* Create two equal columns that floats next to each other */
    .column {
        float: left;
        width: 50%;
        padding: 10px;
    }

        .column img {
            margin-top: 4px;
        }

    /* Clear floats after the columns */
    .row:after {
        content: "";
        display: table;
        clear: both;
    }
</style>
<body>

    <!-- Header -->
    <div class="header">
        <h1>Image Grid</h1>
    </div>

    <!-- Photo Grid -->
    <div class="row">
        <div class="column">
            <img src="~/images/2.jpg" style="width:100%">
            <img src="~/images/ff.jpg" style="width:100%">
         
        </div>
      
        <div class="column">
            <img src="~/images/bzs-2.jpg" style="width:100%">
            <img src="~/images/hqdefault.jpg" style="width:100%">
            
    </div>

</body>

